                />

        </LinearLayout>

    </LinearLayout>










    <ImageView
        android:id="@+id/tiao3"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_below="@id/shangpin2"
        android:background="@drawable/game_default_banner"
        />


    <LinearLayout
        android:id="@+id/shangpin3"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_below="@+id/tiao3"
